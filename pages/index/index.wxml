<!--index.wxml-->
<view class="index">
  <view class="origin-name">
    <text>距 {{origin_name}}</text>
  </view>
  <view class="dis-str">
    <text class="user-motto">{{dis_str}}</text>
  </view>
  <view style="margin-bottom:200rpx;" class="ll-groups">
    <view class="ll">
      <text>当前经度：{{lng}}</text>
    </view>
    <view class="ll">
      <text>当前纬度：{{lat}}</text>
    </view>
  </view>
  <view style="width:80%">
    <button type="primary" bindtap="setOrigin"> 设置原点 </button>
  </view>
</view>
